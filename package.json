{
  "name": "racetrack2",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "serve": "ng serve --proxy-config proxy.conf.json",
    "start": "npm run backend",
    "heroku-postbuild": "npm run ng-environment && npm run backend:build && npm run build:notaot",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "build:notaot": "ng build --aot=false --environment=prod --output-hashing=all --sourcemaps=false --named-chunks=false --extract-css=true --build-optimizer=false",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ng-environment": "node ./node_modules/ng-node-environment/index.js",
    "backend": "node --optimize_for_size --max_old_space_size=920 --gc-interval=100 bin/server/server",
    "backend:build": "npm run backend:clean && tsc --project backend-tsconfig.json",
    "backend:clean": "npm run rimraf -- bin",
    "rimraf": "rimraf"
  },
  "engines": {
    "node": "8.9.1",
    "npm": "5.2.0"
  },
  "private": true,
